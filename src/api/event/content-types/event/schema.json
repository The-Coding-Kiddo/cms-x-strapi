{
  "kind": "collectionType",
  "collectionName": "events",
  "info": {
    "singularName": "event",
    "pluralName": "events",
    "displayName": "event"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "title": {
      "type": "string",
      "required": true
    },
    "description": {
      "type": "text"
    },
    "eventDate": {
      "type": "date",
      "required": true
    },
    "startTime": {
      "type": "time"
    },
    "endTime": {
      "type": "time"
    },
    "location": {
      "type": "string"
    },
    "type": {
      "type": "enumeration",
      "enum": [
        "training",
        " workshop",
        " conference",
        " community",
        " advocacy",
        " networking"
      ]
    },
    "image": {
      "type": "media",
      "multiple": false,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    }
  }
}
